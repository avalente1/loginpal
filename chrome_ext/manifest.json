{
  "manifest_version": 2,



  "name": "LoginPal",
  "description": "One-click password hint",
  "version": "0.2.1",

  "browser_action": {
    "default_icon": "loginpal.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "history",
    "tabs",
    "http://www.loginpal.com/*"
  ],
  "web_accessible_resources": ["script.js"]
}
