<%= form_for site, remote: true, class: "form-inline", id: "form_new" do |f| %>
  <td><%= f.text_field :company, placeholder: "Example: Facebook", id: "company_new" %></td>
  <td><%= f.text_field :username_sb, placeholder: "Username" %></td>
  <td><%= f.text_field :pwhint_sb, placeholder: "Password hint" %></td>
  <td><%= f.button '<i class="icon-plus"></i>'.html_safe, class: "btn btn-success", id: 'new' %></td>
  <th> </th>
<% end %>
